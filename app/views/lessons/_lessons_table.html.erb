<table>
<tr><th>Check In Time</th><th>Student</th><th>Teacher</th><th>Instrument</th><th>Books</th></tr>
<% lessons.each do |lesson| %>
<tr><td><%= lesson.time_in %></td><td><%= lesson.student.first_name %> <%= lesson.student.last_name %></td><td><%= lesson.teacher.first_name %> <%= lesson.teacher.last_name %></td><td><%= lesson.brought_instrument %></td><td><%= lesson.brought_books %></td></tr>
<% end %>
</table>
